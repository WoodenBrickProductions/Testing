 public static void perejimas(int playerx,int playery,int playersize, int Sobjectx, int objecty)
{
  if(playerx>=mWidth) 
  {
      Sobjectx -= mWidth;
    
  }
  if(playerx<=0-playersize) 
  {
      Sobjectx += mWidth;
    
  }
  if(playery>=mHeight) 
  {
      objecty -= mHeight;
    
  }
  if(playery<=0-playersize) 
  {
      objecty += mHeight;
    
  }
}
 
 
 public static void MapMove(char direction) {
   switch(direction) {
    
     case 'd':
   for(Square sq : terrain) {
     sq.xpos -= mWidth;
   }
   break;
   
     case 'a':
   for(Square sq : terrain) {
     sq.xpos += mWidth;
   }
   break;
     
     case 's':
   for(Square sq : terrain) {
     sq.ypos -= mHeight;
   }
   break;
 
     case 'w':
   for(Square sq : terrain) {
     sq.ypos += mHeight;
   }
   break;
 
   } 
 
 
  
